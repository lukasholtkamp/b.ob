<launch>
    <node pkg="joy_linux" exec="joy_linux_node" output="screen"/>
    <node pkg="joy_linux" exec="sub_node" output="screen" />
    <node pkg="joy" exec="joy_node" output="screen" > 
        <param from="$(find-pkg-share joy_linux)/config/BasicDrive.yaml" />
    </node>
    <node pkg="teleop_twist_joy" exec="teleop_node" name="teleop_node" output="screen" >
        <param from="$(find-pkg-share joy_linux)/config/BasicDrive.yaml" />
    </node>
</launch>